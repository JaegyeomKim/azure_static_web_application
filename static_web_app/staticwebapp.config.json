{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/css/*", "/js/*", "/images/*", "/vendor/*"]
  },
  "routes": [
    {
      "route": "/",
      "serve": "/index.html"
    },
    {
      "route": "/services",
      "serve": "/services.html"
    },
    {
      "route": "/about",
      "serve": "/about.html"
    },
    {
      "route": "/contact",
      "serve": "/contact.html"
    },
    {
      "route": "/assets/*",
      "serve": "/assets/"
    },
    {
      "route": "/css/*",
      "serve": "/css/"
    },
    {
      "route": "/js/*",
      "serve": "/js/"
    },
    {
      "route": "/images/*",
      "serve": "/images/"
    },
    {
      "route": "/vendor/*",
      "serve": "/vendor/"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "mimeTypes": {
    ".html": "text/html",
    ".css": "text/css",
    ".js": "application/javascript",
    ".json": "application/json",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".jpeg": "image/jpeg",
    ".svg": "image/svg+xml",
    ".woff": "font/woff",
    ".woff2": "font/woff2",
    ".ttf": "font/ttf"
  },
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
    "Access-Control-Allow-Headers": "Content-Type"
  }
}
